{
  "name": "my-custom-auth",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "my-custom-auth/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/my-custom-auth",
        "main": "libs/my-custom-auth/src/index.ts",
        "tsConfig": "libs/my-custom-auth/tsconfig.lib.json",
        "assets": ["libs/my-custom-auth/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "my-custom-auth/jest.config.ts"
      }
    }
  }
}
